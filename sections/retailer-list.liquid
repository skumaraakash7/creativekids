<head>
    <!-- Link Swiper's CSS -->
    <link
      rel="stylesheet"
      href="https://unpkg.com/swiper/swiper-bundle.min.css"
    />  
</head>
<style>

  .our_brand-section-container {
/*    padding: 20px 0;  */
   background: white; 
   position: relative;    
  }
  .become-retailer-button-container {
   padding-top: 15px;
   max-width: 400px;
   margin: auto;
  }
  button.become-retailer-button {
    width: 100%;
    color: #777154;
    font-size: 15px;
    font-weight: 700;
    padding: 7px;
    border-radius: 30px;
    box-shadow: 2px 2px 6px 0px #000;
    border: 0px;
    background-image: linear-gradient(#ecff00, #f5cc00);
  }
    .mySwiper {
    -webkit-transition-timing-function:linear!important;
	-o-transition-timing-function:linear!important;
	transition-timing-function:linear!important;
  }
  .our_brand-section_image img {
    max-width: 200px;
  }
/*   Swiper Styles */
  .swiper-slide {
    /* Center slide text vertically */
    display: -webkit-box;
    display: -ms-flexbox;
    display: -webkit-flex;
    display: flex;
    -webkit-box-pack: center;
    -ms-flex-pack: center;
    -webkit-justify-content: center;
    justify-content: center;
    -webkit-box-align: center;
    -ms-flex-align: center;
    -webkit-align-items: center;
    align-items: center;
    margin: auto;
  }
  .our_brand-section-image_section.swiper>.swiper-wrapper {
    transition-timing-function: linear;
  }
  .our_brand-section_title { 
    text-align: center;
  }
  . our_brand-section-image_section  {
   padding: 1rem 0; 
  }

</style>
<div class="our_brand-section-container page-width">

<div class="our_brand-section">

<div class="our_brand-section_title">
<h1>{{ section.settings.title | escape }}</h1>
</div> 
  <div class="our_brand-section-image_section swiper">
    <div class="swiper-wrapper">    
        {% for block in section.blocks %}
        <div class="our_brand-section_image swiper-slide" >
            <img src="{{ block.settings.product-image | img_url: 'master' }}" class="swiper-image-retailers">
        </div>
        {% endfor %}
  </div>
</div>

  <div class="become-retailer-button-container">
  <button class="become-retailer-button raise"><a href="{{ section.settings.btn-url }}" style="text-decoration: none;color: #777154;">{{ section.settings.btn-text }}</a></button>
</div>
</div>
    <!-- Swiper JS -->
    <script src="https://unpkg.com/swiper/swiper-bundle.min.js"></script>

    <!-- Initialize Swiper -->
    <script>
      var swiper = new Swiper(".our_brand-section-image_section", {
//      grabCursor: true,
    loop: true,
    autoplay: {
    delay: 0,
    freeMode: true,
    freeModeFluid: true,
    disableOnInteraction: false
  },
    speed: 1200,
	spaceBetween: 0,
	slidesPerView: 'auto',
        breakpoints: {
      320: {
        slidesPerView: 1.2,            
      },
      500: {
        slidesPerView: 2.2,
      },
      650: {
        slidesPerView: 2.5,
      },
      840: {
        slidesPerView: 3,
      },
      1000: {
        slidesPerView: 3.5,
      }          
    }
      });
    </script>
{% schema %}
{
"name": "Retailer List",
"settings": [
{
"type": "text",
"id": "title",
"label": "Title"
},
{
"type": "text",
"id": "btn-text",
"label": "Button text"
},
{
"type": "url",
"id": "btn-url",
"label": "Url For Button"
}
],

"blocks": [
{
"type": "select",
"name": "Add image",
"settings": [
{
"id": "product-image",
"type": "image_picker",
"label": "image picker"
}
]
}
],
"presets": [
{
"name": "Retailer List",
"category": "Custom Sections"
}
]
}
{% endschema %}
