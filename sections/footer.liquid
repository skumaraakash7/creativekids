    }
    .footer__content-top.page-width {
      width: 100%;
      max-width: 100%;
    }
    .brand_logo_1 img {
      max-width: 130px;
    }
    footer.footer.color-inverse {
      padding-top: 20px;
      height: 100%;
    }
